function adjust_char(c, val)
  local addr = 0x5600 + ord(c)\2
  local shft = (ord(c) & 1) * 4
  local mask = 0xf << shft
  poke(addr, (peek(addr) & ~mask) | (val << shft))
end

function load_from_sprites()
	
	--find maximum sprite index
	--(look for any set pixel)
	maxi=0
	for i=0,255 do
	 local x0=(i%16)*8
	 local y0=(i\16)*8
	  for y=0,7 do
	 	 for x=0,7 do
	 	  if(sget(x0+x,y0+y)>0) maxi=i
	 	 end
	 	end
	end
	
	-- grab bits from each sprite
	for i=0,maxi do
	
	 local x0=(i%16)*8
	 local y0=(i\16)*8
	
	 for y=0,7 do
	 	local val=0
	 	for x=0,7 do
	 	 if sget(x0+x,y0+y)>0 then
	 	  val |= (1<<x)
	 	 end
	 	end
	 	poke(0x5600+i*8+y,val)
	 	
	 end
	
	end
	
	-- font attributes are stored
	-- in character 0
	poke(0x5600,
	 char_width,
	 char_width2,
	 char_height,
	 char_offset_x,
	 char_offset_y)
	
	adjust_char("0",1)
	adjust_char("1",7)
	adjust_char("3",1)
	adjust_char("4",1)
	adjust_char("I",7)
	adjust_char("J",7)
	adjust_char("L",7)
	adjust_char("M",2)
	adjust_char("W",2)
	adjust_char("a",1)
	adjust_char("b",2)
	adjust_char("c",1)
	adjust_char("d",1)
	adjust_char("e",1)
	adjust_char("g",1)
	adjust_char("h",2)
	adjust_char("i",7)
	adjust_char("j",7)
	adjust_char("k",1)
	adjust_char("l",1)
	adjust_char("m",2)
	adjust_char("n",1)
	adjust_char("o",1)
	adjust_char("p",2)
	adjust_char("q",1)
	adjust_char("r",2)
	adjust_char("s",1)
	adjust_char("t",1)
	adjust_char("u",1)
	adjust_char("v",1)
	adjust_char("w",2)
	adjust_char("x",1)
	adjust_char("y",1)
	adjust_char("z",2)
	adjust_char(".",6)
	adjust_char("!",7)
	adjust_char("?",1)
	adjust_char("'",7)
	adjust_char(" ",7)

	poke(0x5f58,0x81)--set custom font
	poke(0x5605,1)--variable width

	-- generate string
 local str=""
	
	for i=0,maxi*8+7 do
		str..=chr(peek(0x5600+i))
	end
	
	return "?\"\^!5600"..escbin(str).."\""
end

function escbin(s)
 local out=""
 for i=1,#s do
  local c  = sub(s,i,i)
  local nc = ord(s,i+1)
  local pr = (nc and nc>=48 and nc<=57) and "00" or ""
  local v=c
  if(c=="\"") v="\\\""
  if(c=="\\") v="\\\\"
  if(ord(c)==0) v="\\"..pr.."0"
  if(ord(c)==10) v="\\n"
  if(ord(c)==13) v="\\r"
  out..= v
 end
 return out
end


