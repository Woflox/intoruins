-- size of character (width2
-- is user for chr >= 128)
char_width    = 4
char_width2   = 8
char_height   = 7

-- draw offset
char_offset_x = 0
char_offset_y = 0

function _init()
	memset(0x5600,0,0x800)
	local s=load_from_sprites()
	printh(s,"@clip")
end


function _draw()
cls(1)

poke(0x5f58,0x81)
color(7)
?"the quick'n brown"
?"fox jumps over "
?"the lazy dog."
?""
